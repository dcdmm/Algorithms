   "source": [
    "def moves_ins(n, forward):\n",
    "    if n == 0:\n",
    "        return None\n",
    "    moves_ins(n - 1, True)\n",
    "    print(\"enter \", n) if forward else print(\"exit  \", n)\n",
    "    moves_ins(n - 1, False)"
   ]
  },
